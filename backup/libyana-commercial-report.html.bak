    <script>
        // Animated Counter Function
        function animateCounter(element, targetValue) {
            // Get target value from parameter or data attribute or element text
            let target = targetValue;
            if (!target) {
                if (element.hasAttribute('data-target')) {
                    target = parseFloat(element.getAttribute('data-target'));
                } else {
                    // Extract number from text content
                    const text = element.textContent.trim();
                    target = parseFloat(text.replace(/[^\d.]/g, ''));
                }
            }

            if (isNaN(target)) return;

            // Determine decimals based on original value
            const decimals = (target % 1 !== 0) ? 1 : 0;

            // Extract suffix (like M, K, %, etc.)
            let suffix = '';
            const text = element.textContent.trim();
            const suffixMatch = text.match(/[^\d.,\s]+$/);
            if (suffixMatch) suffix = suffixMatch[0];

            const duration = 1500;
            const increment = target / (duration / 16);
            let current = 0;

            const updateCounter = () => {
                current += increment;
                if (current < target) {
                    let displayValue = current.toFixed(decimals);
                    // Add comma separators for numbers >= 1000
                    if (current >= 1000) {
                        displayValue = displayValue.replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    }
                    element.textContent = displayValue + suffix;
                    requestAnimationFrame(updateCounter);
                } else {
                    let displayValue = target.toFixed(decimals);
                    if (target >= 1000) {
                        displayValue = displayValue.replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    }
                    element.textContent = displayValue + suffix;
                }
            };

            updateCounter();
        }

        // Intersection Observer
        const observerOptions = {
            threshold: 0.2,
            rootMargin: '0px 0px -100px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    // Find all counter elements
                    const counters = entry.target.querySelectorAll('.stat-number, .video-stat-value, .kpi-value[data-target]');
                    counters.forEach(counter => {
                        // Store original value if not already stored
                        if (!counter.hasAttribute('data-original-value')) {
                            counter.setAttribute('data-original-value', counter.textContent.trim());
                        }

                        const originalValue = counter.getAttribute('data-original-value');
                        const numberMatch = originalValue.match(/[\d.,]+/);
                        if (numberMatch) {
                            const targetNumber = parseFloat(numberMatch[0].replace(/,/g, ''));
                            const suffix = originalValue.replace(/[\d.,\s]/g, '');

                            // Reset to 0 and animate
                            counter.textContent = '0' + suffix;
                            animateCounter(counter, targetNumber);
                        }
                    });
                } else {
                    // Reset counters when scrolling away
                    const counters = entry.target.querySelectorAll('.stat-number, .video-stat-value, .kpi-value[data-target]');
                    counters.forEach(counter => {
                        const originalValue = counter.getAttribute('data-original-value');
                        if (originalValue) {
                            const suffix = originalValue.replace(/[\d.,\s]/g, '');
                            counter.textContent = '0' + suffix;
                        }
                    });
                }
            });
        }, observerOptions);

        // Observe all stat showcases
        document.querySelectorAll('.stat-showcase, .video-stats').forEach(el => {
            observer.observe(el);
        });

        // Chart Colors
        const chartColors = {
            primary: '#75197c',
            light: '#8b2591',
            accent: '#9a3d9f',
            gray: '#B2BAC2',
            gridColor: '#4a4d52',
